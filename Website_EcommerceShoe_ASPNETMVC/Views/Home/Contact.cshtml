    <script type="text/javascript">
    window.onload = function () {
        function _hubjsGenerateValue(value, destination) {
            if (destination.indexOf(value) != -1) {
                return true;
            } else {
                return false;
            }
        }
        function _hubjsFilterValueFromFormData(filters, filter_string, value) {
            var field_value = "";
            filters.forEach(element => {
                let isExits = _hubjsGenerateValue(element, filter_string);
                if (isExits) {
                    field_value = value;
                    return;
                }
            });

            return field_value;
        }
        function _hubjsGetCookie(name) {
            var nameEQ = name + "=";
            var ca = document.cookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') c = c.substring(1, c.length);
                if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length, c.length);
            }
            return '';
        }
        function _hubjs(STR_XPATH, dom) {
            var xresult = document.evaluate(STR_XPATH, dom, null, XPathResult.ANY_TYPE, null);
            var xnodes = [];
            var xres;
            while (xres = xresult.iterateNext()) {
                xnodes.push(xres);
            }

            return xnodes;
        }
        function _hubjs_convert_text(xpath_value) {
            if (xpath_value.length > 0) {
                return xpath_value[0].nodeValue;
            } else {
                return "";
            }
        }
        function _hubjsIsRealValue(obj) {
            return obj && obj !== 'null' && obj !== 'undefined';
        }
        $(document).ready(function () {
            // detected params
            var values = {
                'email': '',
                'firstname': '',
                'lastname': '',
                'fullname': '',
                'username': '',
                'mobile': '',
                'birth_day': '',
                'gender': '',
                'is_synchronized': 1,
                'ga': _hubjsGetCookie('_ga'),
                'funnel': window.location.host,
                'hub_js_checkout': {
                    'order_number': '',
                    'total_price': 0,
                    'total_tax': 0,
                    'shipping_rate': 0,
                    'discount': 0,
                    'line_items': []
                }
            };

            // defined sessionStorage
            if (sessionStorage.getItem("contact") == null) {
                sessionStorage.setItem("contact", JSON.stringify(values));
            }

            // array field name
            let fields_email = ["email", "register_email"];
            let fields_mobile = ["phone-number", 'register_phone'];
            let fields_fistname = ["full-name", "register_name"];
            let fields_lastname = [];
            let fields_birth_day = ["birthday", "register_birthday"];
            let fields_gender = ["gender"];
            let fields_username = ["user_name"];

            /********************************************/
            /***** DEVELOPER MUST CHANGE START HERE *****/


            // this is url subdirectory when checkout success
            let checkout_success = 'order-received';

            // this is url subdirectory when access to cart [only one step for checkout]
            let cart_success = '/cart';

            /******* END CHANGE SCRIPT TRACKING *******/
            /******************************************/



            // get infomation cart items
            var current_url = $(location).attr('href');

            if (_hubjsGenerateValue(cart_success, current_url)) {
                values = JSON.parse(sessionStorage.getItem("contact"));
                values.hub_js_checkout.line_items = [];

                let items = _hubjs(product_block, document);

                items.forEach(element => {
                    values.hub_js_checkout.line_items.push({
                        "product_id": Math.floor(Math.random() * 99999) + 1,
                        "title": _hubjs_convert_text(_hubjs(product_name_xpath, element)),
                        "type": "",
                        "price": _hubjs_convert_text(_hubjs(product_price_xpath, element)).replace(/[^0-9]+/g, "").trim(),
                        "quantity": _hubjs_convert_text(_hubjs(product_quantity_xpath, element)).replace(/[^0-9]+/g, "").trim()
                    });
                });
                // save session storage
                sessionStorage.setItem("contact", JSON.stringify(values));
            }

            // submit form
            $("form").on('submit', function (e) {
                // e.preventDefault();

                $.each($(this).serializeArray(), function (i, field) {

                    // convert email
                    let email = _hubjsFilterValueFromFormData(fields_email, field.name, field.value);
                    if (email) {
                        values['email'] = email;
                    }

                    // convert mobile
                    let mobile = _hubjsFilterValueFromFormData(fields_mobile, field.name, field.value);
                    if (mobile) {
                        values['mobile'] = mobile;
                    }

                    // convert username
                    let username = _hubjsFilterValueFromFormData(fields_username, field.name, field.value);
                    if (username) {
                        values['username'] = username;
                    }

                    // convert birth_day
                    let birth_day = _hubjsFilterValueFromFormData(fields_birth_day, field.name, field.value);
                    if (birth_day) {
                        values['birth_day'] = birth_day;
                    }

                    // convert gender
                    let gender = _hubjsFilterValueFromFormData(fields_gender, field.name, field.value);
                    if (gender) {
                        if (gender == "1") {
                            values['gender'] = 'Nam';
                        } else if (gender == "0") {
                            values['gender'] = 'Nữ';
                        }
                    }

                    // convert first_name
                    let firstname = _hubjsFilterValueFromFormData(fields_fistname, field.name, field.value);
                    if (firstname) {
                        values['firstname'] = firstname;
                    }

                    // convert fullname step 01
                    if (values['firstname']) {
                        values['fullname'] = values['firstname'];
                    }

                    // convert last_name
                    let lastname = _hubjsFilterValueFromFormData(fields_lastname, field.name, field.value);
                    if (lastname) {
                        values['lastname'] = lastname;
                    }

                    // convert fullname step 02
                    if (values['lastname']) {
                        if (values['fullname']) {
                            values['fullname'] = values['fullname'] + " " + values['lastname'];
                        } else {
                            values['fullname'] = values['lastname'];
                        }
                    }
                });

                // append line items for values object
                let Hubjs = JSON.parse(sessionStorage.getItem("contact"));

                if (_hubjsIsRealValue(Hubjs) && Hubjs.hub_js_checkout.line_items.length > 0) {
                    values.hub_js_checkout.line_items = Hubjs.hub_js_checkout.line_items;
                }

                // check value not empty
                $.each(values, function(index, element){
                    if(!values[index] || values[index] == null || values[index] == undefined ){
                        delete values[index];
                    }
                });

                // save session storage
                sessionStorage.setItem("contact", JSON.stringify(values));

                if(values['email'] || values['mobile'])
                {
                    // push data to DMP
                    _paq.push(['setUserId', values['mobile']]);
                    _paq.push(['setCustomDimension', 1, values['email']]);
                    _paq.push(['setCustomDimension', 2, values['mobile']]);
                    _paq.push(['setCustomDimension', 3, values['fullname']]);
                    _paq.push(['setCustomDimension', 4, values['birth_day']]);
                    _paq.push(['setCustomDimension', 5, values['ga']]);
                    _paq.push(['setCustomDimension', 6, values['gender']]);

                    // push data to CDP
                    mt('send', 'pageview', values);
                }
                // console.log(values);
                // console.log(JSON.parse(sessionStorage.getItem("contact")));
                return true;
        });

        // debug developer
        //console.log(JSON.parse(sessionStorage.getItem("contact")));

        if (_hubjsGenerateValue(checkout_success, current_url)) {
            let Hubjs = JSON.parse(sessionStorage.getItem("contact"));

            if (_hubjsIsRealValue(Hubjs) && Hubjs.hub_js_checkout.line_items.length > 0) {
                // push product items to DMP
                $.each(Hubjs.hub_js_checkout.line_items, function (i, product) {
                    _paq.push(['addEcommerceItem', product.product_id, product.title, product.type, product.price, product.quantity]);
                });

                // push order infomation to DMP
                let order_number = _hubjs_convert_text(_hubjs(order_code_xpath, document)).trim();
                let subtotal = _hubjs_convert_text(_hubjs(order_subtotal_xpath, document)).replace(/[^0-9]+/g, "").trim();
                let tax = Hubjs.hub_js_checkout.total_tax ? Hubjs.hub_js_checkout.total_tax : "";
                let shipping = _hubjs_convert_text(_hubjs(order_shipping_xpath, document)).replace(/[^0-9]+/g, "").trim();
                let discount = _hubjs_convert_text(_hubjs(order_discount_xpath, document)).replace(/[^0-9]+/g, "").trim();
                let total_revenue = _hubjs_convert_text(_hubjs(order_total_revenue_xpath, document)).replace(/[^0-9]+/g, "").trim();

                if (!discount) {
                    discount = 0;
                } else {
                    discount = parseInt(discount);
                }

                if (!shipping) {
                    shipping = 0;
                } else {
                    shipping = parseInt(shipping);
                }

                if (!subtotal) {
                    subtotal = 0;
                } else {
                    subtotal = parseInt(subtotal);
                }

                if (!total_revenue) {
                    total_revenue = 0;
                } else {
                    total_revenue = parseInt(total_revenue);
                }

                if ((total_revenue - shipping) < subtotal) {
                    discount = subtotal - total_revenue;
                }

                // console.log({
                //     'order_number': order_number,
                //     'total_revenue': total_revenue,
                //     'subtotal': subtotal,
                //     'tax': tax,
                //     'shipping': shipping,
                //     'discount': discount,
                // })

                let userId = '';
                if (Hubjs.email != '') {
                    userId = Hubjs.email;
                } else {
                    userId = Hubjs.mobile;
                }

                _paq.push(['setUserId', userId]);
                _paq.push(['setCustomDimension', 1, Hubjs.email]);
                _paq.push(['setCustomDimension', 2, Hubjs.mobile]);
                _paq.push(['setCustomDimension', 3, Hubjs.fullname]);
                _paq.push(['setCustomDimension', 4, Hubjs.birth_day]);
                _paq.push(['setCustomDimension', 5, Hubjs.ga]);
                _paq.push(['setCustomDimension', 6, Hubjs.gender]);
                _paq.push(['trackEcommerceOrder', order_number, total_revenue, subtotal, tax, shipping, discount]);
            }
        }
    });
    }
